generator client {
  provider = "prisma-client-js"
  output   = "../generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model IPO {
  name               String @id
  date               String
  exchange           String?
  numberOfShares     Int?
  price              String?
  status             String
  symbol             String?
  totalSharesValue   Int?
}


